{
  "name": "electron-test",
  "version": "1.0.0",
  "description": "A minimal Electron application with React",
  "main": "src/main.js",
  "dependencies": {
    "office-ui-fabric-core": "^9.6.1",
    "react": "^16.5.2",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "^2.16.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-electron": "react-app-rewired build --scripts-version react-scripts-ts",
    "electron-dev": "electron . http://localhost:3000",
    "electron-prod": "yarn build-electron && electron ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "electron": "^3.0.4",
    "office-ui-fabric-react": "^6.102.0",
    "react-app-rewired": "^1.6.2",
    "typescript": "^3.1.6"
  }
}
